<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LogsReader">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <TabPane prefHeight="200.0" prefWidth="400.0" side="BOTTOM" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Log">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="425.0" prefWidth="509.0">
                     <children>
                        <TextArea fx:id="logsPlace" prefHeight="335.0" prefWidth="397.0" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </left>
   <right>
      <VBox prefHeight="371.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextFlow prefHeight="270.0" prefWidth="200.0" />
            <Button fx:id="showLogsButton" mnemonicParsing="false" onAction="#buttonClicked" text="Pokaz logi">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Zapisz logi do bazy danych">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" />
               </VBox.margin></Button>
            <Button fx:id="saveLogsButton" mnemonicParsing="false" onAction="#buttonClicked" text="Zapisz do pliku...">
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </right>
</BorderPane>
